{
  "name": "@neabyte/ollama-native",
  "version": "0.2.0",
  "description": "TypeScript client library for Ollama API with retry logic, timeout management, error handling, and validation.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && tsc-alias -f -fe .js && npm run minify",
    "minify": "find dist -name '*.js' -exec terser {} --compress --mangle -o {} \\;",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist && rm -rf .tsbuildinfo",
    "prepublishOnly": "npm run clean && npm run build",
    "type-check": "tsc --noEmit",
    "check-all": "npm run lint && npm run type-check"
  },
  "keywords": [
    "ollama",
    "typescript",
    "client",
    "api",
    "llm",
    "ai",
    "retry",
    "timeout",
    "http-client",
    "nodejs",
    "machine-learning",
    "language-model",
    "chat",
    "generation",
    "streaming",
    "tool-calling",
    "function-calling",
    "model-management",
    "async",
    "promise",
    "fetch",
    "http",
    "rest",
    "sdk",
    "library"
  ],
  "author": {
    "name": "NeaByteLab",
    "email": "me@neabyte.com",
    "url": "https://github.com/NeaByteLab"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NeaByteLab/Ollama-Native.git"
  },
  "bugs": {
    "url": "https://github.com/NeaByteLab/Ollama-Native/issues"
  },
  "homepage": "https://github.com/NeaByteLab/Ollama-Native#readme",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-sonarjs": "^3.0.5",
    "prettier": "^3.6.2",
    "terser": "^5.44.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
